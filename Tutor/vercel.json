{
    "builds": [
        {
            "src": "Tutor/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        
        {
             "src": "/(.*)", 
            "dest": "Tutor/wsgi.py" 
        }
    ]
}